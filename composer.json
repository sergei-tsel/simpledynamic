{
    "name": "sergei-tsel/simpledynamic",
    "type": "project",
    "description": "The skeleton application for the Simpledynamic framework.",
    "keywords": ["simple", "dynamic", "framework"],
    "license": "MIT",
    "require-dev": {
        "vimeo/psalm": "^5.26",
        "symplify/easy-coding-standard": "^12.3.6",
        "rector/rector": "^1.2.7"
    },
    "require": {
        "php": "^8.4",
        "ext-simplexml": "*",
        "ext-yaml": "*",
        "ext-mbstring": "*",
        "laravel/database": "^8.83",
        "doctrine/mongodb-odm": "^2.7",
        "twig/twig": "^3.14",
        "guzzlehttp/guzzle": "^7.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/database/factories/",
            "Database\\Seeders\\": "database/database/seeds/",
            "config\\": "config/"
        }
    }
}
